<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Daftar Mahasiswa</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <div class="container">
      <% if(messages.success) { %>
      <div class="alert alert-info" role="alert"><%- messages.success %></div>
      <% } %>
      <div class="row">
        <div class="col">
          <a href="/mahasiswa/create" class="btn btn-sm btn-success">Tambah</a>
          <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Nama</th>
                <th scope="col">NRP</th>
                <th scope="col">Program Studi</th>
                <th scope="col">tgl lahir</th>
                <th scope="col">jenis kelamin</th>
                <th scope="col">agama</th>
                <th scope="col">hoby</th>
                <th scope="col">alamat</th>
                <th colspan="2" scope="col">Aksi</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i = 0; i < data.length; i++){ %>
              <tr>
                <th scope="row"><%= (i+1) %></th>
                <td><%= data[i].NAMA %></td>
                <td><%= data[i].NRP %></td>
                <td><%= data[i].PROGRAM_STUDY %></td>
                <td><%= data[i].TGL_LAHIR %></td>
                <td><%= data[i].JENIS_KELAMIN %></td>
                <td><%= data[i].AGAMA %></td>
                <td><%= data[i].ALAMAT %></td>
                <td><%= data[i].HOBY %></td>
                

                <td>
                  <a
                    href="/mahasiswa/edit/<%= data[i].id_mahasiswa %>"
                    class="btn btn-sm btn-warning"
                    >Edit</a
                  >
                  <a
                    href="/mahasiswa/delete/<%= data[i].id_mahasiswa %>"
                    class="btn btn-sm btn-danger"
                    >Delete</a
                  >
                </td>
              </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </body>
</html>